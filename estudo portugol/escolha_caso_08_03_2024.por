programa {
  funcao inicio() {

    real notaUm = 1.00
    real notaDois = 0.00

    real soma = notaUm +  notaDois

    escolha(soma){
      caso 1:
      escreva("você tirou um de nota")
      pare
      caso 2:
      escreva("você tirou dois de nota")
      pare
      caso 3:
      escreva("você tirou três de nota")
      pare
      caso 4:
      escreva("você tirou quatro de nota")
      pare
      caso contrario:
      escreva("você tirou cinco de nota")
      pare


    }
    
  }
}
